{
  "name": "eslint-config-vazco",
  "version": "2.1.0",
  "description": "ESLint rules used across Vazco.eu in Meteor/React projects",
  "peerDependencies": {
    "babel-eslint": "^7.0.0",
    "eslint": "^3.8.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-vazco": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "eslint": "^3.8.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-vazco": "^1.0.0",
    "yamljs": "^0.2.8"
  },
  "main": "default.json",
  "files": [
    "*.json"
  ],
  "scripts": {
    "test": "eslint tests/empty.js -c configs/default.yaml",
    "build": "yaml2json -s configs && mv -v configs/*.json .",
    "prepublish": "npm test && npm run build"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "vazco",
    "meteor"
  ],
  "author": {
    "name": "Maciek Stasieluk",
